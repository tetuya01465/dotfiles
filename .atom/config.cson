"*":
  autosave:
    enabled: true
  core:
    closeDeletedFileTabs: true
    disabledPackages: [
      "spell-check"
    ]
    telemetryConsent: "no"
  editor:
    preferredLineLength: 1000
    showIndentGuide: true
    showInvisibles: true
  "exception-reporting":
    userId: "46ee98a6-e0de-4874-bc1e-dd381de91e7d"
  fonts:
    fontFamily: "FiraCode"
  "linter-ui-default":
    showPanel: true
  "terminal-plus":
    core: {}
  welcome:
    showOnStartup: false
